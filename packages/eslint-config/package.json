{
  "name": "@react-esm-native-microfrontends/eslint-config",
  "version": "1.0.0",
  "description": "Common ESLint config for react-esm-native-microfrontends monorepo",
  "main": "index.js",
  "scripts": {
    "format": "npm run format:prettier",
    "format:prettier": "npm run lint:prettier -- --write",
    "lint": "npm run lint:prettier",
    "lint:prettier": "prettier \"**/*.{json,js,ts,tsx}\" --check --ignore-path ../../.prettierignore",
    "clean": "rimraf .turbo dist node_modules",
    "debug:prettier": "prettier \"./index.js\" --loglevel debug --ignore-path ../../.prettierignore"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "eslint-plugin-unicorn": "51.0.1"
  }
}
